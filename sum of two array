#include<stdio.h>
int main()
{
    int sum=0,a[2]={1,2},b[2]={3,4},c[100],i,j,n,m,sum1=0;
    for(i=0;i<2;i++)
    {
    //scanf("%d",&a[i]);
    sum=sum+a[i];
    }
    for(j=0;j<2;j++)
    {
      //scanf("%d",&b[j]);
        sum1=sum1+b[j];
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
    {
        c[i,j]=sum+sum1;
    }
    }
    for(i=0;i<1;i++)
    {
        for(j=0;j<1;j++)
        
            printf("The sum is %d",c[i,j]);
        
    }
}
